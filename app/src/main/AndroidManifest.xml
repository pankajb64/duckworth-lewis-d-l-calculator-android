<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.fl.dlc">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".activity.DLMainActivity"
            android:label="@string/app_name"
            android:parentActivityName=".activity.DLMainActivity">
        <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.SuspensionsActivity"
            android:label="@string/title_activity_suspensions">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".activity.DLMainActivity" />
        </activity>
    </application>

</manifest>
